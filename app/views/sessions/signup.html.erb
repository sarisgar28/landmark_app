<h1>Please Signup:</h1>

<%= form_with model: @user, url: signup_path do |f| %>
    <%= f.label :username%>
    <%= f.text_field :username%>

    <%= f.label :email %>
    <%= f.email_field :email %>
  
    <%= f.label :password %>
    <%= f.password_field :password %>
  
    <%= f.submit "Signup" %>
  
<% end %>