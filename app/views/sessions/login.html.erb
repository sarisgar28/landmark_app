<h1>Please Login</h1>

<%= form_with model: @user, url: login_path do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Login" %>

<% end %>

<%= link_to "Google Login", "/auth/google_oauth2" %>
<%= link_to "Home", root_path %>
